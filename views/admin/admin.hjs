                                                <th>操作</th>
                                            </thead>
                                            <tbody class="usersBody">
                                            </tbody>
                                        </table>
                                        <script>
                                            $.ajax({
                                                    method: 'get',
                                                    url: '/api/allUsers',
                                                    success: function(data) {
                                                        for (var i = 0; i < data.length; i++) {
                                                            $('.usersBody').append(`
                                                                    <tr>
                                                                        <td>${i+1}</td>
                                                                        <td>${data[i].username}</td>
                                                                        <td>${data[i].created_at}</td>
                                                                        <td><a onclick="deleteUser('${data[i]._id}', this)" href="#">删除</a></td>
                                                                    </tr>
                                                                `)
                                                        }
                                                    },
                                                    error: function(error) {
                                                        console.log(error)
                                                    }
                                                })
                                                //delete listing button
                                            function deleteUser(id, el) {
                                                $.ajax({
                                                    method: 'get',
                                                    url: '/api/users/delete/' + id,
                                                    success: function(data) {
                                                        $(el).parent().parent().remove();
                                                        alert(`用户已删除`);
                                                    },
                                                    error: function(data) {
                                                        alert('删除失败')
                                                    }
                                                })
                                            }
                                        </script>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="content video-tab">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="card">
                                    <div class="header">
                                        <h4 class="title">视频管理</h4>
                                        <p class="category">添加，删除或管理网站视频区内容</p>
                                    </div>
                                    <div>
                                        <label>Youtube 视频名</label>
                                        <input type="text" name="videoName" id="videoName">
                                        <label>URL</label>
                                        <input type="text" name="Url連結" id="videoUrl">
                                        <button id="videoSubmit">發布</button>
                                    </div>
                                    <script type="text/javascript">
                                        $.get('/api/videos/fetch/list', (data) => {
                                            for (var i = 0; i < data.length; i++) {
                                                $('#videoList').append(`
                                                    <tr>
                                                        <td>${data[i].videoName}</td>
                                                        <td>${data[i].videoUrl}</td>
                                                        <td>${data[i].created_at}</td>
                                                        <td><a href="#" onclick="deleteVideo('${data[i]._id}', this)">删除</a></td>
                                                    </tr>
                                                `)
                                            }
                                        })
                                        $('#videoSubmit').click(() => {
                                            var videoName = $('#videoName').val()
                                            var videoUrl = $('#videoUrl').val()
                                            $.ajax({
                                                method: 'post',
                                                url: '/api/videos/new',
                                                data: {
                                                    videoName,
                                                    videoUrl,
                                                },
                                                success: function(data) {
                                                    alert('上传成功！')
                                                    $('#videoList').append(`
                                                        <tr>
                                                            <td>${data.videoName}</td>
                                                            <td>${data.videoUrl}</td>
                                                            <td>${data.created_at}</td>
                                                            <td><a href="#" onclick="deleteVideo('${data._id}', this)">删除</a></td>
                                                        </tr>
                                                    `)
                                                },
                                                error: function(err) {
                                                    alert('上传失败！')
                                                }
                                            })
                                        })

                                        function deleteVideo(id, el) {
                                            $.get(`/api/videos/delete/${id}`, (data) => {
                                                alert('删除成功');
                                                $(el).parent().parent().remove();
                                            })
                                        }
                                    </script>
                                    <div class="content table-responsive table-full-width">
                                        <table class="table table-hover table-striped">
                                            <thead>
                                                <th>视频名</th>
                                                <th>URL</th>
                                                <th>發布日期</th>
                                                <th>操作</th>
                                            </thead>
                                            <tbody id="videoList">
                                            </tbody>
                                        </table>

                                    </div>
                                </div>
                            </div>
                        </div>
