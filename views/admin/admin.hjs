                                        <h4 class="title">分类广告管理</h4>
                                        <p class="category">查询或删除分类广告</p>
                                    </div>
                                    <div class="filterBlock">
                                        <select class="listing-state">
                                                <option selected value="xw">地区</option>
                                            </select>
                                        <select class="listing-category">
                                                <option selected value="xw">分类</option>
                                            </select>
                                        <button class="btn btn-large btn-info" id="searchListing">搜索</button>
                                    </div>
                                    <div class="content table-responsive table-full-width">
                                        <table class="table table-hover table-striped">
                                            <thead>
                                                <th>ID</th>
                                                <th>标题</th>
                                                <th>发布日期</th>
                                                <th>点击量</th>
                                                <th>类型</th>
                                                <th>操作</th>
                                            </thead>
                                            <tbody class="listingBody">
                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- //显示所有分类 -->
                                    <script>
                                        $.get('/api/states/en/fetch', function(statesEN) {
                                            $.get('/api/states/cn/fetch', function(statesCN) {
                                                for (var i = 0; i < statesEN.length; i++) {
                                                    $('.listing-state').append(`
                                                            <option value="${statesEN[i]}">${statesCN[i]}</option>
                                                        `)
                                                }
                                            })
                                        })
                                        $.get('/api/category/en/fetch', function(en) {
                                            $.get('/api/category/cn/fetch', function(cn) {
                                                for (var i = 0; i < en.length; i++) {
                                                    $('.listing-category').append(`
                                                            <option value="${en[i]}">${cn[i]}</option>
                                                        `)
                                                }
                                            })
                                        })
                                        $('#searchListing').click(function() {
                                                var state = $('.listing-state').val()
                                                var category = $('.listing-category').val()
                                                $('.listingBody').empty();
                                                $.get(`/api/listings/fetchAll/${state}/${category}`, (data) => {
                                                    for (var i = 0; i < data.length; i++) {
                                                        $('.listingBody').append(`
                                                            <tr>
                                                                <td>${data[i]._id}</td>
                                                                <td>${data[i].data.title}</td>
                                                                <td>${data[i].created_at}</td>
                                                                <td>${data[i].views}</td>
                                                                <td>${data[i].data.type}</td>
                                                                <td><a href="#" onclick="deleteListing(${data[i]._id})">删除</a></td>
                                                            </tr>
                                                        `)
                                                    }
                                                })
                                            })
                                            //get all listing
                                        $.get(`/api/listings/fetch`, (data) => {
                                            for (var i = 0; i < data.length; i++) {
                                                $('.listingBody').append(`
                                                            <tr>
                                                                <td>${data[i]._id}</td>
                                                                <td>${data[i].data.title}</td>
                                                                <td>${data[i].created_at}</td>
                                                                <td>${data[i].views}</td>
                                                                <td>${data[i].data.category}</td>
                                                                <td><a onclick="deleteListing('${data[i]._id}', this)" href="#">删除</a></td>
                                                            </tr>
                                                        `)
                                            }
                                        })

                                        //delete listing button
                                        function deleteListing(id, el) {
                                            $.ajax({
                                                method: 'get',
                                                url: '/api/listing/delete/' + id,
                                                success: function(data) {
                                                    console.log($(el))
                                                    $(el).parent().parent().remove();
                                                    alert(`信息已删除`);
                                                },
                                                error: function(data) {
                                                    alert('删除失败')
                                                }
                                            })
                                        }
                                    </script>
                                </div>
