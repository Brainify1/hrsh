                        //get news list for management
                        $.get({
                                url: '/api/news/fetch',
                                success: function(data) {
                                    for (var i = 0; i < data.length; i++) {
                                        $('.newsBody').append(`
                                            <tr>
                                                <td>${data[i]._id}</td>
                                                <td>${data[i].title}</td>
                                                <td>${data[i].created_at}</td>
                                                <td>${data[i].views}</td>
                                                <td>
                                                    <a href="#" data-api="/api/deleteNews" data-id="${data[i]._id}" class="deleteNews" onclick="deleteNews('${data[i]._id}', this)">删除</a>
                                                </td>
                                            </tr>
                                        `)
                                    }
                                }
                            })
                            //delete news button
                        function deleteNews(id, el) {
                            $.ajax({
                                method: 'post',
                                url: '/api/news/delete',
                                data: {
                                    id: id
                                },
                                success: function(data) {
                                    alert(`新闻已删除`);
